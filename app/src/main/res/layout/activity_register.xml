<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.RegisterActivity">

    <EditText
        android:fontFamily="@font/sourcesanspro_light"
        android:id="@+id/textUsuario2"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/ic_cuadro"
        android:ems="10"
        android:hint="Nombre de Usuario"
        android:inputType="textPersonName"
        android:paddingLeft="50dp"
        app:layout_constraintEnd_toEndOf="@+id/textViewSesion4"
        app:layout_constraintStart_toStartOf="@+id/textViewSesion4"
        app:layout_constraintTop_toBottomOf="@+id/textViewSesion4">

    </EditText>
"

    <Button

        android:id="@+id/buttonCancelar"

        android:layout_width="121dp"
        android:layout_height="52dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="200dp"
        android:background="@drawable/ic_fondolleno"
        android:fontFamily="@font/sourcesanspro_semibold"
        android:text="CANCELAR"
        android:textColor="@color/colorAccent"
        app:layout_constraintEnd_toEndOf="@+id/editTextCorreo"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/editTextCorreo"
        app:layout_constraintTop_toBottomOf="@+id/editTextCorreo" />

    <Button
        android:id="@+id/buttonIngresar"

        android:layout_width="121dp"
        android:layout_height="52dp"
        android:layout_marginStart="200dp"
        android:layout_marginTop="132dp"
        android:background="@drawable/ic_fondolleno"
        android:fontFamily="@font/sourcesanspro_semibold"
        android:text="INGRESAR"
        android:textColor="@color/colorAccent"
        app:layout_constraintEnd_toEndOf="@+id/editTextCorreo"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="@+id/imageView12"
        app:layout_constraintTop_toBottomOf="@+id/editTextCorreo" />

    <EditText
        android:fontFamily="@font/sourcesanspro_light"
        android:id="@+id/editTextContraseña"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/ic_cuadro"
        android:ems="10"
        android:hint="Contraseña"
        android:inputType="textPassword"
        android:paddingLeft="50dp"
        android:paddingTop="4dp"
        app:layout_constraintEnd_toEndOf="@+id/textUsuario2"
        app:layout_constraintStart_toStartOf="@+id/textUsuario2"
        app:layout_constraintTop_toBottomOf="@+id/textUsuario2" />

    <EditText
        android:fontFamily="@font/sourcesanspro_light"
        android:id="@+id/editTextContraseña2"
        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/ic_cuadro"
        android:ems="10"
        android:hint="Confirmar contraseña"
        android:inputType="textPassword"
        android:paddingLeft="50dp"
        android:paddingTop="4dp"
        app:layout_constraintEnd_toEndOf="@+id/editTextContraseña"
        app:layout_constraintStart_toStartOf="@+id/editTextContraseña"
        app:layout_constraintTop_toBottomOf="@+id/editTextContraseña" />

    <ImageView

        android:id="@+id/icono"
        android:layout_width="19dp"
        android:layout_height="21dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/textUsuario2"
        app:layout_constraintStart_toStartOf="@+id/textUsuario2"
        app:layout_constraintTop_toTopOf="@+id/textUsuario2"
        app:layout_constraintVertical_bias="0.517"
        app:srcCompat="@drawable/ic_usuariologo" />

    <TextView
        android:id="@+id/textViewSesion4"


        android:layout_width="101dp"
        android:layout_height="26dp"
        android:layout_marginTop="12dp"
        android:background="@color/colorAccent"
        android:fontFamily="@font/sourcesanspro_regular"
        android:paddingLeft="10dp"
        android:text="Registrate"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="253dp"
        android:layout_height="70dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_bio_logo" />

    <EditText
        android:fontFamily="@font/sourcesanspro_light"
        android:id="@+id/editTextCorreo"

        android:layout_width="307dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/ic_cuadro"
        android:ems="10"
        android:hint="Correo electronico"
        android:inputType="textEmailAddress"
        android:paddingLeft="50dp"
        android:paddingTop="0dp"
        app:layout_constraintEnd_toEndOf="@+id/editTextContraseña2"
        app:layout_constraintStart_toStartOf="@+id/editTextContraseña2"
        app:layout_constraintTop_toBottomOf="@+id/editTextContraseña2" />

    <ImageView
        android:id="@+id/candado"
        android:layout_width="19dp"
        android:layout_height="21dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextContraseña"
        app:layout_constraintStart_toStartOf="@+id/editTextContraseña"
        app:layout_constraintTop_toTopOf="@+id/editTextContraseña"
        app:layout_constraintVertical_bias="0.517"
        app:srcCompat="@drawable/ic_candado" />

    <ImageView
        android:id="@+id/candado2"
        android:layout_width="19dp"
        android:layout_height="21dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextContraseña2"
        app:layout_constraintStart_toStartOf="@+id/editTextContraseña2"
        app:layout_constraintTop_toTopOf="@+id/editTextContraseña2"
        app:layout_constraintVertical_bias="0.517"
        app:srcCompat="@drawable/ic_candado" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="19dp"
        android:layout_height="21dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/editTextCorreo"
        app:layout_constraintStart_toStartOf="@+id/editTextCorreo"
        app:layout_constraintTop_toTopOf="@+id/editTextCorreo"
        app:layout_constraintVertical_bias="0.517"
        app:srcCompat="@drawable/ic_correo" />

    <com.facebook.login.widget.LoginButton
        android:id="@+id/buttonFacebookLogin"
        android:layout_width="100dp"
        android:layout_height="27dp"
        android:layout_marginTop="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonCancelar" />

    <EditText
        android:id="@+id/txtCelular"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="phone"
        android:hint="Telefono"
        app:layout_constraintEnd_toEndOf="@+id/editTextCorreo"
        app:layout_constraintStart_toStartOf="@+id/editTextCorreo"
        app:layout_constraintTop_toBottomOf="@+id/editTextCorreo" />

    <EditText
        android:id="@+id/txtFecha"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="date"
        android:hint="Fecha de nacimiento"
        app:layout_constraintEnd_toEndOf="@+id/txtCelular"
        app:layout_constraintStart_toStartOf="@+id/txtCelular"
        app:layout_constraintTop_toBottomOf="@+id/txtCelular" /><![CDATA[





    "
        android:layout_marginTop="30dp"
        android:layout_marginBottom="30dp" />



]]>
</androidx.constraintlayout.widget.ConstraintLayout>